package edu.codepath.todoapp;

public class ToDoItem
{
	private String todo;
	/***** Do not Need
	private Date date;
	private Date due;
	private boolean status;
	**/
	
	public ToDoItem(String todo)
	{
		this.todo=todo;
	}
	public String getTodo()
	{
		return todo;
	}
